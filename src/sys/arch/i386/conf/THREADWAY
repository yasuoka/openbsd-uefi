option		SWAPPAGER
option		VNODEPAGER
option		DEVPAGER

option		DDB
#makeoptions	DEBUG="-g"
option		DIAGNOSTIC
option		KTRACE

option		SYSVMSG
option		SYSVSEM
option		SYSVSHM
#option		SHMMAXPGS=1024

option		COMPAT_NOMID
