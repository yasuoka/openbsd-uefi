machine		octeon mips64
include		"../../../conf/GENERIC"
maxusers	32

option		CPU_OCTEON
makeoption	LINK_ADDRESS="0xffffffff81000000"

option		PCIVERBOSE
option		USBVERBOSE

config	bsd	swap generic

#
# Definition of system
#
mainbus0	at root
cpu0		at mainbus0
clock0		at mainbus0
obio0		at mainbus0

com0		at obio0
com1		at obio0

option		COM_OCTEON

