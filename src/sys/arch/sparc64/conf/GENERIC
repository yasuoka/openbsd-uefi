machine		sparc64

include "../../../conf/GENERIC"

option		SUN4U

option		PCIVERBOSE
option		USER_PCICONF	# user-space PCI configuration
option		APERTURE
option		USBVERBOSE

maxusers	64

config		bsd	swap generic

# Main bus and CPU .. all systems.
mainbus0 at root
