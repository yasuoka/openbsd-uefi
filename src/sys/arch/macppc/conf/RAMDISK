ehci*		at pci?	disable	# Enhanced Host Controller
ehci*		at cardbus? disable # Enhanced Host Controller

# USB bus support
usb*		at ohci?	flags 0x1
usb*		at ehci?
uhub*		at usb?
uhub*		at uhub?

uhidev*		at uhub?
